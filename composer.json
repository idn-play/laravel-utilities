{
    "name": "idn-play/laravel-utilities",
    "license": "proprietary",
    "description": "Laravel utilities",
    "authors": [
        {
            "name": "IDN PLAY",
            "email": "azam@idnplay.com"
        }
    ],
    "type": "helper",
    "minimum-stability": "dev",
    "autoload": {
        "files": [
            "src/Helpers/autoload.php"
        ],
        "psr-4": {
            "IdnPlay\\Laravel\\Utils\\": "src/"
        }
    },
    "require": {
        "mobiledetect/mobiledetectlib": "^2.8",
        "nesbot/carbon": "^2.16",
        "illuminate/support": "^6 || ^7 || ^8",
        "illuminate/database": "^6 || ^7 || ^8",
        "illuminate/http": "^6 || ^7 || ^8",
        "illuminate/redis": "^6 || ^7 || ^8"
    },
    "extra": {
        "laravel": {
            "providers": [
                "IdnPlay\\Laravel\\Utils\\Providers\\ServiceProvider"
            ]
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    },
    "prefer-stable": true
}
